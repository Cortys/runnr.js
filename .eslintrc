{
	"env": {
		"node": true,
		"es6": true,
		"mocha": true,
		"builtin": true
	},
	"globals": {},
	"rules": {
		"block-scoped-var": 2,
		"camelcase": 2,
		"comma-dangle": [2, "never"],
		"comma-style": [2, "last"],
		"curly": [2, "multi"],
		"dot-notation": [2, {
			"allowKeywords": true
		}],
		"eqeqeq": [2, "allow-null"],
		"guard-for-in": 2,
		"new-cap": 2,
		"no-caller": 2,
		"no-cond-assign": [2, "except-parens"],
		"no-debugger": 2,
		"no-empty": 2,
		"no-eval": 2,
		"no-extend-native": 2,
		"no-extra-parens": 2,
		"no-irregular-whitespace": 2,
		"no-iterator": 2,
		"no-loop-func": 2,
		"no-multi-str": 2,
		"no-new": 2,
		"no-script-url": 2,
		"no-sequences": 2,
		"no-undef": 2,
		"no-with": 2,
		"quotes": [2, "double"],
		"semi": [2, "always"],
		"no-extra-semi": 2,
		"strict": [2, "global"],
		"valid-typeof": 2,
		"wrap-iife": [2, "inside"]
	}
}
